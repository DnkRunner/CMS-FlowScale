    {
      "src": "/admin/pages",
      "dest": "/admin/pages.html"
    },
    {
      "src": "/admin/media",
      "dest": "/admin/media.html"
    },
    {
      "src": "/admin/comments",
      "dest": "/admin/comments.html"
    },
    {
      "src": "/admin/categories",
      "dest": "/admin/categories.html"
    },
    {
      "src": "/admin/users",
      "dest": "/admin/users.html"
    },
    {
      "src": "/admin/settings",
      "dest": "/admin/settings.html"
